  <div class="form">
    <%= simple_form_for [@service, @booking] do |f| %>
      <%= f.input :start_date, as: :string, required: false, input_html: {id: "range_start"} %>
      <%= f.input :end_date, as: :string, required: false, input_html: {id: "secondRangeInput"} %>

